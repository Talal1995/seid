{
  "name": "seid",
  "version": "1.0.0",
  "description": "A product website with React frontend and Node.js backend",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "server": "nodemon server/server.js",
    "client": "npm start --prefix client",
