    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "mongoose": "^7.5.3",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-scripts": "5.0.1",
    "styled-components": "^6.1.16",
    "webpack": "4.29.6"
  },
  "devDependencies": {
    "babel-jest": "27.5.1",
    "babel-loader": "8.0.5",
    "concurrently": "^8.2.1",
    "eslint": "7.32.0",
    "jest": "27.5.1",
